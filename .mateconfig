PI=3.14159265359
GOLDEN_RATIO = 1.61803398875
e=2.71828182846

min2(x,y) = (x<y) * x + (x>=y) * y
abs(x) = (x>=0) * x + (x<0) * (-x)
sign(x) = (x>0) - (x<0)  
clamp(x,min,max) = min2(max2(x,min),max)  

max2(x,y) = (x>y) * x + (x<=y) * y
pow(x,y) = x^y
sqrt(x) = x^0.5
cube_root(x) = x^(1/3)
hypot(x,y) = (x^2 + y^2)^0.5  

deg_to_rad(d) = d * PI / 180
rad_to_deg(r) = r * 180 / PI

square_area(s) = s^2
circle_area(r) = PI*r^2 
circun(r) = 2*PI*r
rectangle_area(w,h) = w*h
triangle_area(b,h) = 0.5*b*h
sphere_volume(r) = (4/3)*PI*r^3
sphere_surface(r) = 4*PI*r^2
cylinder_volume(r,h) = PI*r^2*h
cone_volume(r,h) = (1/3)*PI*r^2*h
